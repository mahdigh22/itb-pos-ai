{
    "rewrites": [
      { "source": "/manifest.json", "destination": "/manifest.json" },
      { "source": "/sw.js", "destination": "/sw.js" }
    ],
    "headers": [
      {
        "source": "/(manifest.json|sw.js)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }
  